<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
              http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
	<navigation-rule>
		<description>Home</description>
		<display-name>Home</display-name>
		<from-view-id/>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/jsf/home/home.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<description>DataTable SelectionTest</description>
		<display-name>DataTable Selection</display-name>
		<from-view-id/>
		<navigation-case>
			<from-outcome>dtSelectionTest</from-outcome>
			<to-view-id>/jsf/datatable/selectiontest.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<description>requiredTest</description>
		<display-name>requiredTest</display-name>
		<from-view-id/>
		<navigation-case>
			<from-outcome>requiredTest</from-outcome>
			<to-view-id>/jsf/requiredTest/requiredTest.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<description>inputNumberTabViewTest</description>
		<display-name>inputNumberTabViewTest</display-name>
		<from-view-id/>
		<navigation-case>
			<from-outcome>inputNumberTabViewTest</from-outcome>
			<to-view-id>/jsf/inputNumber/tabView.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<description>selectOneMenue</description>
		<display-name>selectOneMenueTest</display-name>
		<from-view-id/>
		<navigation-case>
			<from-outcome>selectOneMenue</from-outcome>
			<to-view-id>/jsf/selectOneMenue/selectOneMenue.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
</faces-config>